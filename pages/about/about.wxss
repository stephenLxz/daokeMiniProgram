/* pages/about/about.wxss */
Page{
  background-color: #ffffff;
}
.middle {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 300rpx 0;
  /* background-color: #fff; */
}

.logo {
  width: 160rpx;
  height: 160rpx;
  border-radius: 30rpx;
  overflow: hidden;
}

.logo image {
  width: 100%;
  height: 100%;
}
.middle .logo-title{
  padding: 70rpx 0 50rpx 0;
  font-size: 42rpx;
}
.company{
  font-size: 35rpx;
  color: #999999;
}
.version{
  position: absolute;
  font-size: 35rpx;
  color: #999999;
  bottom:80rpx;
}
